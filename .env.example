# ModalMuse Environment Variables
# Copy this file to .env and fill in your values

# === REQUIRED ===

# LlamaParse API Key (get from cloud.llamaindex.ai)
LLAMA_PARSE_API_KEY=llx-your-key-here

# Groq API Key (get from console.groq.com)
GROQ_API_KEY=gsk_your-key-here

# Jina AI API Key (get from jina.ai)
JINA_API_KEY=jina_your-key-here

# Supabase Configuration (get from app.supabase.com)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key-here

# Qdrant Cloud (get from cloud.qdrant.io)
QDRANT_URL=https://your-cluster.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key

# === OPTIONAL ===

# Image Storage Configuration
URL_BASED_IMAGE_INDEXING=false
SUPABASE_STORAGE_ENABLED=true

# CORS Origins (comma-separated for multiple)
CORS_ORIGINS=https://your-frontend.vercel.app,http://localhost:3000

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
